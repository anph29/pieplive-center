#:kivy 1.10.1
#File name: piepme.py

<MainView>:
    mainStream: mainStream
    right_content: right_content
    bottom_left:bottom_left
    control:control
    listMixer: bottom_left.list_mixer
    canvas.before:
        Color:
            rgb:.12, .15, .18
        Rectangle:
            pos: self.pos
            size: self.size
    BoxLayout:#MainWrapper
        orientation:'horizontal'
        size_hint: None,None
        size: root.width, root.height
        cols:2
        pos: 0,0
        BoxLayout:#LeftContent
            size_hint_x: None
            size_hint_y: 1
            width:root.width - 375
            spacing:5
            padding:5

            Box11:#TopLeft
                size_hint:1,.8
                AnchorLayout:
                    size_hint:1, None
                    height: (self.parent.height - 40)
                    canvas.before:
                        Color:
                            rgb:.12, .15, .18
                        Rectangle:
                            pos: self.pos
                            size: self.size
                    MainStream:
                        id: mainStream
                        size_hint:1, 1
                        anchor_x: 'center'
                        anchor_y: 'center'
                      
                Control:
                    id: control
                    size_hint: 1, None
                    height: 40
                
            BottomLeft:
                id:bottom_left
                size_hint:1,.2
                rows:1
                cols:3
                spacing:5

        RightContent:
            id:right_content
        
