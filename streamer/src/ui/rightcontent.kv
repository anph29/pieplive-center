#:import helper src.utils.helper
<RightContent>:
    tab_media:tab_media
    tab_image:tab_image
    tab_audio:tab_audio
    tab_camera:tab_camera
    tab_presenter:tab_presenter
    tab_schedule:tab_schedule
    tab_width: self.size[0] / 6
    size_hint:1,1
    pos_hint:{'y': 0, 'center_x': .5}
    do_default_tab: False

    TabbedPanelItem:
        text: 'Audio'
        Box25:
            padding:2
            TabAudio:
                id:tab_audio
                canvas.before:
                    Color:
                        rgb: .0,.0,.0
                    Rectangle:
                        pos: self.pos
                        size: self.size

    TabbedPanelItem:
        text: 'Image'
        Box25:
            padding:2
            TabImage:
                id:tab_image
                canvas.before:
                    Color:
                        rgb: .0,.0,.0
                    Rectangle:
                        pos: self.pos
                        size: self.size

    TabbedPanelItem:
        text: 'Video'
        Box25:
            padding:2
            TabMedia:
                id:tab_media
                canvas.before:
                    Color:
                        rgb: .0,.0,.0
                    Rectangle:
                        pos: self.pos
                        size: self.size

    TabbedPanelItem:
        text: 'Camera'
        Box25:
            padding:2
            TabCamera:
                id:tab_camera
                canvas.before:
                    Color:
                        rgb: .0,.0,.0
                    Rectangle:
                        pos: self.pos
                        size: self.size
            
    TabbedPanelItem:
        text:'Presenter'
        Box25:
            padding:2
            TabPresenter:
                id:tab_presenter
                f_root:root
                canvas.before:
                    Color:
                        rgb: .0,.0,.0
                    Rectangle:
                        pos: self.pos
                        size: self.size
    TabbedPanelItem:
        text:'Schedule'
        Box25:
            padding:2
            TabSchedule:
                id:tab_schedule
                f_root:root
                canvas.before:
                    Color:
                        rgb: .0,.0,.0
                    Rectangle:
                        pos: self.pos
                        size: self.size

<TabMedia>:
    ls_media:ls_media
    ListMedia:
        id:ls_media
        viewclass: 'ItemLabel'
        size_hint: 1,None
        height: int(self.parent.height - 30)
        scroll_type: ['bars', 'content']
        bar_width: 10
        GridCamera:
            cols:1
            spacing:1
            padding:1
            default_size_hint: 1, None
            default_size: None, 30
            multiselect: False
            touch_multiselect: False
            size_hint_y: None
            height: self.minimum_height

    BoxLayout:
        orientation:'horizontal'
        size_hint:1, None
        height:30
        padding:5
        spacing:8
        canvas.before:
            Color:
                rgb:.20, .20, .20
            Rectangle:
                pos: self.pos
                size: self.size
        Widget:
        ImageButton:
            source: helper._ICONS_PATH + 'add-w36.png'
            size_hint:None,None
            size: 20,20
            on_release: root.open_add_media()
            tooltip_text:'Add'
        ImageButton:
            source: helper._ICONS_PATH + 'f5W.png'
            size_hint:None,None
            size: 20,20
            on_release: ls_media.refresh_list()
            tooltip_text:'Refresh'
        ImageButton:
            source: helper._ICONS_PATH + 'trash.png'
            size_hint:None,None
            size: 20,20
            on_release: ls_media.remove_selected()
            tooltip_text:'Delete the selected'
            
