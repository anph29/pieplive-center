<TabPresenter>:
    ls_presenter:ls_presenter
    ListPresenter:
        id:ls_presenter
        viewclass: 'ItemPresenter'
        size_hint: 1,None
        height: int(self.parent.height - 30)
        scroll_type: ['bars', 'content']
        bar_width: 10
        GridCamera:
            cols:1
            spacing:1
            padding:1
            default_size_hint: 1, None
            default_size: None, 30
            multiselect: False
            touch_multiselect: False
            size_hint_y: None
            height: self.minimum_height

    BoxLayout:
        orientation:'horizontal'
        size_hint:1, None
        height:30
        padding:5
        spacing:8
        canvas.before:
            Color:
                rgb:.20, .20, .20
            Rectangle:
                pos: self.pos
                size: self.size
        Widget:
            size_hint:1,1
        ImageButton:
            source: helper._ICONS_PATH + 'f5W.png'
            size_hint:None,None
            size: 20,20
            on_release: ls_presenter.refresh_list()
            tooltip_text:'Refresh'
        CheckBox:
            size_hint:None,1
            width: 20
            active:False
            on_active: ls_presenter.change_presenter_auto(self.active)
            canvas.before:
                Color:
                    rgb: 1,1,1
                Rectangle:
                    pos:self.center_x-8, self.center_y-8
                    size:[16,16]
                Color:
                    rgb: 0,0,0
                Rectangle:
                    pos:self.center_x-7, self.center_y-7
                    size:[14,14]
        Label:
            size_hint:None,1
            color: 1.0,1.0,1.0,1
            font_size: 12
            padding: 2,0
            text: 'Auto'
            width: 30
            valign: 'center'
            halign: 'left'
            markup: True

<TabSchedule>:
    ls_schedule:ls_schedule
    ListSchedule:
        id:ls_schedule
        viewclass: 'ItemSchedule'
        size_hint: 1,None
        height: int(self.parent.height - 30)
        scroll_type: ['bars', 'content']
        bar_width: 10
        multiselect: False
        touch_multiselect: False
        GridCamera:
            cols:1
            spacing:1
            padding:1
            default_size_hint: 1, None
            default_size: None, 30
            multiselect: False
            touch_multiselect: False
            size_hint_y: None
            height: self.minimum_height

    BoxLayout:
        orientation:'horizontal'
        size_hint:1, None
        height:30
        padding:5
        spacing:0
        canvas.before:
            Color:
                rgb:.20, .20, .20
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            size_hint:None,1
            color: 1.0,1.0,1.0,1
            font_size: 12
            text: 'Total time: '+ls_schedule.total_time+' '
            valign: 'center'
            halign: 'right'
            markup: True
        Widget:
            size_hint:1,1
        ImageButton:
            source: helper._ICONS_PATH + 'play-w.png'
            size_hint:None,None
            size: 0,0
            on_release: root.f_root.play_schedule(self)
            tooltip_text:'Add'
        ImageButton:
            source: helper._ICONS_PATH + 'f5W.png'
            size_hint:None,None
            size: 20,20
            on_release: ls_schedule.refresh_list()
            tooltip_text:'Refresh'
        Widget:
            size_hint:None,1
            width: 2
        ImageButton:
            source: helper._ICONS_PATH + 'trash.png'
            size_hint:None,None
            size: 20,20
            on_release: ls_schedule.remove_selected()
            tooltip_text:'Delete the selected'
        Widget:
            size_hint:None,1
            width: 8
        CheckBox:
            size_hint:None,1
            width: 20
            active:True
            on_active: root.f_root.change_schedule_loop(self.active)
            canvas.before:
                Color:
                    rgb: 1,1,1
                Rectangle:
                    pos:self.center_x-8, self.center_y-8
                    size:[16,16]
                Color:
                    rgb: 0,0,0
                Rectangle:
                    pos:self.center_x-7, self.center_y-7
                    size:[14,14]
        Label:
            size_hint:None,1
            color: 1.0,1.0,1.0,1
            font_size: 12
            padding: 2,0
            text: 'Loop'
            width: 30
            valign: 'center'
            halign: 'left'
            markup: True